package com.shop.vo;

import java.sql.Date;

public class ProductVO {
	private String prodCd;
	private String prodNm;
	private String ctgCd;
	private String ctgCdRef;
	private int csmePrice;
	private int qty;
	private String rmark;
	private String prodImg;
	private String insUser; 
	private Date insDt;
	private String  updUser;
	
	public String getProdCd() {
		return prodCd;
	}
	public void setProdCd(String prodCd) {
		this.prodCd = prodCd;
	}
	public String getProdNm() {
		return prodNm;
	}
	public void setProdNm(String prodNm) {
		this.prodNm = prodNm;
	}
	public String getCtgCd() {
		return ctgCd;
	}
	public void setCtgCd(String ctgCd) {
		this.ctgCd = ctgCd;
	}
	public int getCsmePrice() {
		return csmePrice;
	}
	public void setCsmePrice(int csmePrice) {
		this.csmePrice = csmePrice;
	}
	public int getQty() {
		return qty;
	}
	public void setQty(int qty) {
		this.qty = qty;
	}
	public String getRmark() {
		return rmark;
	}
	public void setRmark(String rmark) {
		this.rmark = rmark;
	}
	public String getProdImg() {
		return prodImg;
	}
	public void setProdImg(String prodImg) {
		this.prodImg = prodImg;
	}
	public String getInsUser() {
		return insUser;
	}
	public void setInsUser(String insUser) {
		this.insUser = insUser;
	}
	public Date getInsDt() {
		return insDt;
	}
	public void setInsDt(Date insDt) {
		this.insDt = insDt;
	}
	public String getUpdUser() {
		return updUser;
	}
	public void setUpdUser(String updUser) {
		this.updUser = updUser;
	}
	public String getCtgCdRef() {
		return ctgCdRef;
	}
	public void setCtgCdRef(String ctgCdRef) {
		this.ctgCdRef = ctgCdRef;
	}
}
